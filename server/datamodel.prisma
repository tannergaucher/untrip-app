type User {
  id: ID! @id
  email: String! @unique
  password: String!
  isEmailSubscriber: Boolean @default(value: false)
  lists: [List!]
}

type List {
  id: ID! @id 
  title: String!
  places: [Place!]
  user: User!
}

type Place {
  id: ID! @id
  sanityId: String
  name: String
  imageUrl: String
  slug: String
  lat: Float
  lng: Float
  list: List
}

type EmailSubscriber {
  id: ID! @id 
  email: String! @unique
}



